{
  "manifest_version": 3,
  "name": "ReplaceImage",
  "version": "1.0",
  "description": "Replaces all images on a page with a term of your liking!.",
  "permissions": ["activeTab", "scripting", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_title": "Replacify!",
    "default_popup": "popup.html"
  }
}